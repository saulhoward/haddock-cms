<?xml version="1.0" encoding="UTF-8"?>
<config>
	<haddock-project-organisation>
		<config-manager>PublicHTML_ConfigManager</config-manager>
	</haddock-project-organisation>
	<page-elements>
		<titles>
			<separator><![CDATA[&nbsp;&gt;&nbsp;]]></separator>
		</titles>
	</page-elements>
	<pages>
		<error>
			<title>
				<ending value="Errors" />
			</title>
		</error>
	</pages>
	<error-messages>
		<page-not-found>Sorry, the page you requested does not exist.</page-not-found>
	</error-messages>
	<oo-page-classes>
		<exception value="PublicHTML_ExceptionPage" />
	</oo-page-classes>
	<verbosity>
		<exceptions>
			<stack-traces-are-printed value="TRUE" />
		</exceptions>
	</verbosity>
</config>
